{
  "name": "@ljo-hamburg/gutenberg-image-selector",
  "version": "0.1.2",
  "description": "Image Selector Control for Gutenberg",
  "module": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
